        app:layout_constraintStart_toStartOf="@+id/guideline13"
        app:layout_constraintTop_toBottomOf="@+id/buttonTan"
        app:secondFunction="@integer/f_root_y"
        app:secondText="y√¯" />

    <net.halman.numerio.NumerioButton
        android:id="@+id/buttonLP"
