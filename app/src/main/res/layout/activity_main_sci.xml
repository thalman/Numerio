        app:layout_constraintTop_toBottomOf="@+id/buttonSin"
        app:secondFunction="@integer/f_e_pow_x"
        app:secondText="eˣ">

    </net.halman.numerio.NumerioButton>

    <net.halman.numerio.NumerioButton
        android:id="@+id/buttonLog"
